{
  "name": "@stainless-api/release-please",
  "version": "0.1.0",
  "description": "Generate release PRs based on the conventionalcommits.org spec",
  "main": "./build/src/index.js",
  "types": "build/src/index.d.ts",
  "bin": "./build/src/bin/release-please.js",
  "packageManager": "pnpm@8.8.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "tsc",
    "install": "tsc",
    "lint": "gts check",
    "fix": "gts fix",
    "start": "ts-node ./src/bin/release-please.ts",
    "test": "cross-env ENVIRONMENT=test mocha --require test/_fetch-polyfill.js",
    "test:snap": "SNAPSHOT_UPDATE=1 LC_ALL=en pnpm test"
  },
  "files": [
    "build/src",
    "templates",
    "schemas"
  ],
  "repository": "stainless-api/release-please",
  "keywords": [
    "release",
    "conventional-commits"
  ],
  "author": "Stainless Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stainless-api/release-please/issues"
  },
  "homepage": "https://github.com/stainless-api/release-please#readme",
  "config": {
    "snap-shot-it": {
      "sortSnapshots": true
    }
  },
  "devDependencies": {
    "@octokit/types": "^9.0.0",
    "@types/chai": "^4.1.7",
    "@types/diff": "^5.0.2",
    "@types/iarna__toml": "^2.0.1",
    "@types/js-yaml": "^4.0.0",
    "@types/jsonpath": "^0.2.0",
    "@types/lerna__collect-updates": "^5.1.0",
    "@types/lerna__package": "^5.1.0",
    "@types/lerna__package-graph": "^5.1.0",
    "@types/lerna__run-topologically": "^5.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.4",
    "@types/pino": "^7.0.0",
    "@types/semver": "^7.0.0",
    "@types/sinon": "^10.0.0",
    "@types/xmldom": "^0.1.31",
    "@types/yargs": "^17.0.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "c8": "^8.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "gts": "^3.1.0",
    "mocha": "^9.2.2",
    "nock": "^13.0.0",
    "sinon": "15.2.0",
    "snap-shot-it": "^7.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2"
  },
  "dependencies": {
    "@conventional-commits/parser": "^0.4.1",
    "@google-automations/git-file-utils": "^1.2.5",
    "@iarna/toml": "^3.0.0",
    "@lerna/collect-updates": "^6.4.1",
    "@lerna/package": "^6.4.1",
    "@lerna/package-graph": "^6.4.1",
    "@lerna/run-topologically": "^6.4.1",
    "@octokit/core": "^4.0.0",
    "@octokit/graphql": "^5.0.0",
    "@octokit/plugin-retry": "5.0.5",
    "@octokit/plugin-throttling": "6.1.0",
    "@octokit/request": "^6.0.0",
    "@octokit/request-error": "^3.0.0",
    "@octokit/rest": "^19.0.0",
    "@types/npm-package-arg": "^6.1.0",
    "@xmldom/xmldom": "^0.8.4",
    "chalk": "^4.0.0",
    "code-suggester": "^4.2.0",
    "conventional-changelog-conventionalcommits": "^6.0.0",
    "conventional-changelog-writer": "^6.0.0",
    "conventional-commits-filter": "^3.0.0",
    "detect-indent": "^6.1.0",
    "diff": "^5.0.0",
    "figures": "^3.0.0",
