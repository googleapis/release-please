    "node-fetch": "^2.7.0",
    "node-html-parser": "^6.0.0",
    "octokit": "3.1.0",
    "parse-github-repo-url": "^1.4.1",
    "semver": "^7.0.0",
    "type-fest": "^3.0.0",
    "typescript": "^5.2.2",
    "unist-util-visit": "^2.0.3",
    "unist-util-visit-parents": "^3.1.1",
    "xpath": "^0.0.32",
    "yaml": "^2.2.2",
    "yargs": "^17.0.0"
  }
}